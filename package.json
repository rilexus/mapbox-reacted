{
  "name": "mapbox-reacted",
  "version": "0.1.2",
  "author": "Stanislav Panchenko <mail@stanislavpanchenko.de>",
  "license": "MIT",
  "main": "lib/index.js",
  "sideEffects": false,
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "examples": "webpack-dev-server --config ./example/webpack.config.js",
    "clean": "rimraf lib es dist ts-build",
    "type-check": "tsc --noEmit",
    "lint": "tslint ./src/*",
    "validate": "tslint ./src/*",
    "prettify": " prettier --write --config ./.prettierrc ./src/*",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:ts && npm-run-all --parallel build:umd build:es build:cjs build:umd:min && rimraf ts-build",
    "build:ts": "tsc",
    "build:umd": "cross-env BABEL_ENV=rollup NODE_ENV=development rollup -c",
    "build:es": "cross-env BABEL_ENV=jsnext babel src --out-dir es --extensions \".ts,.tsx\"",
    "build:cjs": "babel src --out-dir lib --extensions \".ts,.tsx\"",
    "build:umd:min": "cross-env BABEL_ENV=rollup NODE_ENV=production rollup -c"
  },
  "files": [
    "dist/*",
    "es/*",
    "lib/*"
  ],
  "dependencies": {
    "uuid": "3.3.3"
  },
  "peerDependencies": {
    "mapbox-gl": "^1.3.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-proto-to-assign": "7.5.5",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/plugin-transform-strict-mode": "7.2.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "7.6.2",
    "@types/jest": "24.0.18",
    "@types/mapbox-gl": "^0.54.3",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/uuid": "^3.4.5",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-dev-expression": "0.2.2",
    "cross-env": "6.0.0",
    "file-loader": "4.2.0",
    "ghooks": "^2.0.4",
    "jest": "24.9.0",
    "mapbox-gl": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "3.0.0",
    "rollup": "^1.22.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "rollup-plugin-uglify": "6.0.3",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.6.3",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rilexus/mapbox-reacted.git"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
